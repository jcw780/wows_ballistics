          "no-unused-expressions": "off",
          "no-restricted-globals": "off",
          "no-undef": "off",
          "import/no-amd": "off"
        }
      }
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "reactSnap": {
    "inlineCss": false
  },
  "resolutions": {
    "eslint-webpack-plugin": "2.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
